{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
      "outDir": "../../out-tsc/lib",
      "target": "es2022",
      "declaration": true,
      "declarationMap": true,
      "inlineSources": true,
      "types": [],
      "lib": [
        "dom",
        "es2022"
      ]
